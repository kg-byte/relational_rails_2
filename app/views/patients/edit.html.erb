<%= form_for @patient, url: "/patients/#{@patient.id}", method: :patch, local:true do |f|%>
	<%= f.label :name %>
	<%= f.text_area :name %>
	<%= f.label :age %>
	<%= f.text_area :age %>
	<%= f.label :insurance_carrier %>
	<%= f.text_area :insurance_carrier %>
	<%= f.label :referred_by %>
	<%= f.text_area :referred_by %>
	<%= f.label :referred_by_another_patient %>
	<%= f.check_box :referred_by_another_patient %>
	<%= f.submit "Update" %>
<% end %>

